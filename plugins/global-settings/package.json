{
    "name": "@titan-reactor-plugins/global-settings",
    "version": "1.0.0",
    "description": "Global Settings",
    "author": "Dark Matter",
    "keywords": [
        "titan-reactor-plugin"
    ],
    "peerDependencies": {
        "titan-reactor": "0.5.0"
    },
    "config": {
        "system": {
            "permissions": [
                "settings.write"
            ]
        },
        "global": {
            "folder": "Audio",
            "label": "Global Volume",
            "value": 1,
            "min": 0,
            "max": 1
        },
        "music": {
            "folder": "Audio",
            "label": "Music Volume",
            "value": 0.15,
            "min": 0,
            "max": 1
        },
        "sound": {
            "folder": "Audio",
            "label": "Sound Volume",
            "value": 0.62,
            "min": 0,
            "max": 1
        }
    }
}