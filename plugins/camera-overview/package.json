{
    "name": "@titan-reactor-plugins/camera-overview",
    "version": "2.0.1",
    "description": "Overview Camera Mode",
    "author": "Dark Matter",
    "keywords": [
        "titan-reactor-plugin"
    ],
    "peerDependencies": {
        "titan-reactor-api": "2.0.0"
    },
    "config": {
        "system": {
            "customHooks": [
                "onCustomPIPEntered",
                "onCustomPIPExited",
                "onCustomExitScene"
            ]
        },
        "pipSize": {
            "label": "PIP Size",
            "value": 300,
            "min": 100,
            "max": 400,
            "step": 25
        },
        "fullScreenPIP": {
            "label": "Full Screen PIP",
            "value": false
        },
        "polarRotation": {
            "label": "Polar Rotation",
            "value": 10,
            "min": 4,
            "max": 10,
            "step": 0.5
        },
        "animateTransition": {
            "label": "Animation Transition",
            "value": false
        }
    }
}